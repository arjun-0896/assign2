<div class="page-header"><h1>Photos</h1></div>

<div class="media">
  <table align="center">
    <tr>
      <td><h4>Tour Id</h4></td>
      <td><h4>Photo</h4></td>
    </tr>
      <% @photo.each do |photo| %>
      <tr>
        <td>
          <%= photo.tour_id %>
        </td>
        <td>
          <!-- Renders image -->
          <%= render photo %>
        </td>
     </tr>
    <% end %>
  </table>
</div>
</table>
<div class="field" align="center">
  <%= link_to 'Back', '/agentoptions', :style =>'text-decoration: none;' %>
</div>


